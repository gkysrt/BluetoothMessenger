QToolTip
{
    background-color:rgb(192, 192, 192);
    color: rgb(64, 64, 64);
}

QPushButton{
    background-color:rgb(222, 222, 222);
    height:26px;
    color:rgb(64, 64, 64);
    border-radius: 13px;
   /* qproperty-icon: none;*/
    outline:none;
}

QPushButton:hover{
    background-color: rgb(240, 240, 240);
}

QPushButton:pressed{
    background-color: rgb(170, 170, 170);
}

QPushButton:disabled{
    background-color: rgb(140, 140, 140);
}

QPushButton[listHeader = "true"] {
    background-color:rgb(200, 200, 200);
    color:rgb(64, 64, 64);
    border-radius: 20px;
    outline:none;
}

QPushButton[listHeader = "true"]:hover{
    background-color: rgb(240, 240, 240);
}

QPushButton[listHeader = "true"]:pressed{
    background-color: rgb(170, 170, 170);
}

QPushButton[listHeader = "true"]:disabled{
    background-color: rgb(140, 140, 140);
}

QTabWidget
{
    min-width: 25px;
    min-height: 25px;
    background-color: rgb(64, 64, 64);
    border: 1px solid rgb(64, 64, 64);
    border-radius: 4px;
    font-size:14px;
    /*qproperty-icon: none;*/
}

QTabWidget QWidget
{
    background-color: rgb(222, 222, 222);
    border-bottom-left-radius: 6px;
    border-bottom-right-radius: 6px;
}

QTabWidget::pane {
    border-style: none;
	border-width: 0px;
	border: 0px solid red;
/*	border-radius: 15px; */
    border-top: 0px solid rgb(50, 50, 50);
}

QTabWidget::pane QWidget {
    border-radius: 4px;
}

QTabWidget QTabBar {
    background-color: rgb(200, 200, 200);
    border: 0px solid rgb(50, 50, 50);
    outline:none;

}

QTabWidget QTabBar::tab {
	width: 150px;
    height: 25px;
    border-top-left-radius: 6px;
    border-top-right-radius: 6px;
    color: rgb(64, 64, 64);
    background-color: rgb(192, 192, 192);
    border-top: 1px solid rgb(192, 192, 192);
    border-bottom: 1px solid rgb(192, 192, 192);
	text-align:left;
	font-size: 14px;
	outline:none;
}

QTabWidget QTabBar::tab:selected {
    margin-top:1px;

	color: rgb(40, 144, 229);
	background-color:rgb(222, 222, 222);
	border: 0px;
	/* border-top: 1px solid rgb(40, 144, 229); */
    width: 150px;
    height: 25px;
}

QTabWidget QTabBar::tab:hover:!selected {
	color: rgb(64, 64, 64);
	background-color:rgb(210, 210, 210);
	/* border-top: 1px solid rgb(128, 128, 128); */
	width: 150px;
    height: 25px;
}

QTabWidget QToolTip
{
    background-color:rgb(64, 64, 64);
    color: rgb(192, 192, 192);
}

QLineEdit
{
    border: 1px solid rgb(64, 64, 64);
    color:rgb(64, 64, 64);
    border-radius: 2px;
    margin-left: 8px;
}

QComboBox
{
    background-color: rgb(222, 222, 222);
}


